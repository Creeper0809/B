const NUM_A = 10;
const NUM_B = 20;

func add(x, y) {
    return x + y;
}

func sub(x, y) {
    return x - y;
}

func mul(x, y) {
    return x * y;
}

func div(x, y) {
    return x / y;
}

func test_compare_eq(a, b) {
    if (a == b) {
        return 100;
    }
    return 0;
}

func test_compare_ne(a, b) {
    if (a != b) {
        return 200;
    }
    return 0;
}

func test_compare_lt(a, b) {
    if (a < b) {
        return 300;
    }
    return 0;
}

func test_compare_gt(a, b) {
    if (a > b) {
        return 400;
    }
    return 0;
}

func test_compare_le(a, b) {
    if (a <= b) {
        return 500;
    }
    return 0;
}

func test_compare_ge(a, b) {
    if (a >= b) {
        return 600;
    }
    return 0;
}

func test_while() {
    var i;
    var sum;
    i = 0;
    sum = 0;
    while (i < 5) {
        sum = sum + i;
        i = i + 1;
    }
    return sum;
}

func test_if_else(n) {
    if (n == 0) {
        return 1;
    } else {
        return 2;
    }
}

func main() {
    var total;
    total = 0;
    
    total = total + add(10, 5);
    total = total + sub(20, 8);
    total = total + mul(3, 4);
    total = total + div(100, 10);
    
    total = total + test_compare_eq(5, 5);
    total = total + test_compare_ne(5, 3);
    total = total + test_compare_lt(3, 5);
    total = total + test_compare_gt(10, 5);
    total = total + test_compare_le(5, 5);
    total = total + test_compare_ge(5, 5);
    
    total = total + test_while();
    total = total + test_if_else(0);
    total = total + test_if_else(1);
    
    total = total + NUM_A;
    total = total + NUM_B;
    
    return total;
}
